{
  "name": "experiment-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "open": "^8.4.2",
    "pm2": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.1",
    "react-scripts": "^3.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
  "start": "PORT=3000 NODE_OPTIONS=--openssl-legacy-provider react-app-rewired start",
  "build": "NODE_OPTIONS=--openssl-legacy-provider react-app-rewired build",
  "test": "NODE_OPTIONS=--openssl-legacy-provider react-app-rewired test",
  "eject": "react-scripts eject",
  "pkg": "pkg . --targets node14-linux-x64,node14-macos-x64,node14-win-x64 --out-path=dist"
},
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "customize-cra": "^1.0.0",
    "pkg": "^5.8.1",
    "react-app-rewired": "^2.2.1"
  },
  "bin": "server/server.js",
  "pkg": {
    "assets": [
      "build/**/*",
      "public/**/*"
    ],
    "targets": [
      "node14-linux-x64",
      "node14-macos-x64",
      "node14-win-x64"
    ],
    "options": {
      "ignore": [
        "node_modules/**/test",
        "node_modules/**/docs",
        "node_modules/**/example",
        "node_modules/**/README.md",
        "node_modules/**/CHANGELOG.md",
        "node_modules/**/LICENSE"
      ]
    }
  }
}
